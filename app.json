{
  "expo": {
    "name": "invoiceReaderApp",
    "slug": "invoiceReaderApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "bold": ["assets/fonts/OpenSans-Bold.ttf"],
          "semi-bold": ["assets/fonts/OpenSans-Regular.ttf"],
          "regular": ["assets/fonts/OpenSans-SemiBold.ttf"]
        }
      ]
    ]
  }
}
